{
  "name": "npm-build",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "nodemon server.js --ignore app/js/",
    "build-scss": "node-sass --output-style compressed -o web app/scss",
    "watch-scss": "nodemon -e scss -x \"npm run build-scss\"",
    "build-js": "webpack -p",
    "watch-js": "webpack -d --watch",
    "build": "parallelshell \"npm run build-scss\" \"npm run build-js\"",
    "watch": "parallelshell \"npm run watch-scss\" \"npm run watch-js\"",
    "dev": "parallelshell \"npm run server\" \"npm run watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.14.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.2.1"
  },
  "author": "Joe Slater",
  "license": "MIT"
}
