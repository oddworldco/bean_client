{
  "name": "npm-build",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
      "node": "4.6.1"
    },
  "scripts": {
    "start": "nodemon server.js --ignore app/js/",
    "build-scss": "node-sass --output-style compressed -o web app/scss",
    "watch-scss": "nodemon -e scss -x \"npm run build-scss\"",
    "build-js": "webpack -p",
    "watch-js": "webpack -d --watch",
    "build": "parallelshell \"npm run build-scss\" \"npm run build-js\"",
    "watch": "parallelshell \"npm run watch-scss\" \"npm run watch-js\"",
    "dev": "parallelshell \"npm run server\" \"npm run watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "ble-bean": "^2.1.1",
    "ble-bean-stream": "^0.1.0",
    "express": "^4.14.1",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "stream": "0.0.2",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.6.1",
    "nodemon": "1.12.1",
    "parallelshell": "2.0.0",
    "react-hot-loader": "3.1.3",
    "webpack": "2.7.0"
  },
  "author": "Gaby Ruiz-Funes",
  "license": "MIT",
  "optionalDependencies": {
    "bufferutil": "^3.0.1",
    "utf-8-validate": "^3.0.2"
  }
}
